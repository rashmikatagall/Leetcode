class Solution {
    public int[] getConcatenation(int[] nums) {
        
        
        int[] result = new int[nums.length*2];
        
        int i=0;
        for(int num :nums)
        {
            result[i] =num;
            result[nums.length+i] = num;
            i++;
        }
        
        return result;
        
    }
}
